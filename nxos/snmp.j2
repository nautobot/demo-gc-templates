snmp-server contact {{ config_context["snmp"]["contact"] }}
snmp-server location {{ config_context["snmp"]["location"] }}
{% for host in config_context["snmp"]["host"] %}
snmp-server host {{ host["ip"] }} traps version {{ host["version"] }} {{ host["community"] }}
{% endfor %}
{% for community in config_context["snmp"]["community"] %}
snmp-server community {{ community["name"] }} group {{ community["role"] }}
{% endfor %}

